{
  "name": "Stamina Banner Remover",
  "description": "Remove banner",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "content_scripts": [
    {
      "js": ["js/content.js"],
      "matches": ["https://stamina-online.com/**"]
    }
  ],
  "action": {
    "default_popup": "html/popup.html"
  },
  "permissions": ["scripting", "activeTab"]
}
